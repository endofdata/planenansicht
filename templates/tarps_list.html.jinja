<!DOCTYPE html>
<html lang="de">

<head>
	<title>Planen</title>
</head>

<body>
	<table>
		<thead>
			<tr>
				<th>Nr.</th>
				<th>Anmerkung</th>
				<th>Kategorie</th>
				<th>Typ</th>
			</tr>
		</thead>
		<tbody>
			{% for tarp in tarp_list %}
			<tr>
				<td>{{ tarp.number }}</td>
				<td>{{ tarp.annotation }}</td>
				<td>{{ tarp.category.name }}</td>
				<td>{{ tarp.category.tarp_type.name }}</td>
			</tr>
			{% endfor %}
		</tbody>
	</table>
</body>

</html>